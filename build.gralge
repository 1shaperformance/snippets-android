buildscript {
  repositories {
    jcenter()
    mavenLocal()
    google()
  }
}

dependencies {
  classpath 'com.android.tools.build:gradle:2.3.2'
  classpath 'com.google.gms:google-services:3.1.0'
}

allprojects {
  repositories {
    jcenter()
    google()
    mavenLocal()
  }
}

task clean(type: Delete) {
  delete rootProject.buildDir
}

  
